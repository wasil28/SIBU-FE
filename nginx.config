server {
  listen 80;

  location /tte-bssn/_nuxt/ {
    proxy_pass http://localhost:3000/tte-bssn/_nuxt/;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header X-Forwarded-Proto $scheme;
    proxy_redirect off;
    access_log off;
  }

  location /tte-bssn/ {
    proxy_pass http://localhost:3000/tte-bssn/;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header X-Forwarded-Proto $scheme;
    proxy_redirect off;
  }
}
